  <version>0.0.0</version>
  <description>ROS2 node to convert AWSIM MGRS pose to NavSatFix and publish Doppler velocity in ECEF.</description>
  <maintainer email="rgdacs@todo.todo">rgdacs</maintainer>
  <license>TODO: License declaration</license>

  <buildtool_depend>ament_cmake</buildtool_depend> <depend>rclcpp</depend>
  <depend>geometry_msgs</depend>
  <depend>sensor_msgs</depend>
  <depend>llh_converter</depend>
  <depend>nav_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <build_depend>eagleye_coordinate</build_depend>
  <build_export_depend>eagleye_coordinate</build_export_depend>
